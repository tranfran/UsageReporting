Company_Name = c.execute('select * from Company_Table where Company_Name in ("SOBEYS")')
data = c.fetchall()
for v in data:
    try:
        print(v[0])
        create_Supplier_excel(v[0],
                              datetime.now().month - 3,
                              datetime.now().month - 2,
                              datetime.now().month - 1,
                              18)
    except:
        pass

conn.commit()
conn.close()
